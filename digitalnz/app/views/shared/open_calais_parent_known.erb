<% content_for :header do%>
<%set_title "#{@clazz.to_s.pluralize},  page #{@page} of #{@n_pages} - Geoparsed Records"%>
<%= render :partial => 'archive_searches/search_form_small' %>
<%end%>

<% content_for :col1 do%>
	<div id="content">
		<h1>Open Calais - <%=@calais_parent_word.word.pluralize%></h1>
		<ul id="natlibPropertyList">
			
		<%for property in @properties%>
			<li>
				<%= link_to property.name, calais_child_url(:child_permalink => property.permalink, :parent_permalink => @calais_parent_word.permalink)%>
</li>
		<%end%>
			
		</ul>
	</div>
	
	<%= will_paginate @page_results %>
	
<%end%>

<% content_for :col2 do%>
<%= render :partial => 'shared/geoparsedinfo/'+@clazz.to_s.tableize.singularize+'.erb'%>
<%end%>