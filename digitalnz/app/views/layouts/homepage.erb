<div class="colmask holygrail"> 
    <div class="colmid"> 
        <div class="colleft"> 
            <div class="col1wrap"> 
                <div class="col1"> 
					<!-- Column 1 start --> 
							<div class="indentedContent"> 
							<form action="http://digitalnzgeoparser.tripodtravel.co.nz/archive_searches/search" class="new_archive_search" id="new_archive_search" method="post"><div style="margin:0;padding:0"><input name="authenticity_token" type="hidden" value="9kWxewJJfi6u5mM6amxPMFgQQFGUaLvCoc0HG3gErak=" /></div> 
							  <p> 
							    <input id="archive_search_search_text" name="archive_search[search_text]" class="input" size="30" type="text" /> 
 
							    <input id="archive_search_submit" name="commit" class="super button" type="submit" value="Search Â»" /> 
							  </p> 
							</form> 
						</div> 
					
						<div class="fromTheArchive"> 
							<div class="slideshow" id="slideshow1">
							<%for nl in @random_images%>
										<a href="/map/<%=nl.permalink%>" title="<%=nl.title%>">
										<img src="<%=nl.thumbnail_url%>" alt="<%=nl.title%>" /> 
							            </a>
							<%end%>
								</div> 
						</div> 
					
					
					
				
				<!-- Column 1 end --> 
                </div> 
            </div> 
            <div class="col2"> 
				<!-- Column 2 start --> 
				<div id="map"> 
