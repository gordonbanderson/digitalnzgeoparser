<h1>Already Geoparsed</h1>


<ul>
<%for result in @natlib_records
metarecord = result
%>
<li class="searchResult">
    <a href="<%=result.landing_url%>"><%=result.title%></a>
    <br/>
<%if !result.thumbnail_url.blank?%><img src="<%=result.thumbnail_url%>" class="searchResultImage"/><%end%>

<p><%=metarecord.description%></p>
<div class="searchByLine">
    <%=hostof(result.landing_url)%> - 
<%=result.content_partner%> - 
<%#result.category%> - 
<%#result.dateinfo%> - 
<%=link_to "Similar Pages", search_archive_searches_url(:q=>result.title)%> -
<%if metarecord.pending %>
Not Geoparsed
<%elsif !metarecord.submission.blank?%>
<%=link_to 'Map', map_natlib_metadata_url(:id=>metarecord.natlib_id)%>
<%else%>
<%=link_to 'Metadata', map_natlib_metadata_url(:id=>metarecord.natlib_id)%>
<%end%>
&nbsp;<%=result.natlib_id%> 
</div>
</li>
<%end%>
</ul>

<%= will_paginate @natlib_records %>